#compdef fnew

typeset -A opt_args

projectList=("${(@f)$(fnew -l 2>/dev/null)}")

function _fnew {
    _arguments '-h[prints help]' \
    '-l[lists the available fnew projects]' \
    '-u[updates the fnew project list]' \
     '1:project:->projects' \

    case "$state" in
      (projects)
          _describe -t projectList 'command' projectList && ret=0
      ;;
      (generator_lists)
    esac

    return 1
}